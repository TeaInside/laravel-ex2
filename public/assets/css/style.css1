/*Dashboard*/
.panel {
    background-color: #FFFFFF;
    /*display: inline-block;*/
}
.panel-default > .panel-heading {
    background-color: #F5F5F5;
    border-color: #DDDDDD;
    color: #333333;
}
.panel-default .panel-heading {
    color: #333333;
    font-size: 1.6em;
    font-weight: 400;
}
.panel-default {
    border-color: #DDDDDD;
}
.panel-body:after{clear:none;}
#dashboard .form-horizontal .form-group:after{clear: none;}
#form_transfer div.col-lg-10 {
width: 68%;
margin-bottom: 10px;
}
#dashboard .panel-charts .table td{text-align:initial;}
#dashboard .panel-charts .table tr td:first-child{text-align:right;}
.text-high{font-weight: bold; font-size: 15px;}

#balances table tr td:first-child{text-align:left;}
#balances .has_amount{color: #2a9fd6;}
.table-striped tbody > tr:nth-child(odd) > td, .table-striped tbody > tr:nth-child(odd) > th {
background-color: #f9f9f9;
}

span.logo {
	display: block;
	padding: 5px;
	color: #ffffff;
	background-color: #248fc1;
	font-size: 14px;
	text-shadow: 0px 1px #133F9E;
	border-bottom: 1px solid #ffffff;
}

#contentinner .generaltext{padding: 20px 10px;}
#contentinner .generaltext h4 {
	font-weight: bold;
	margin: 0;
}

.notifyjs-corner {
position: fixed;
margin: 5px;
z-index: 1050;
}

.notifyjs-corner .notifyjs-wrapper{
position: relative;
display: block;
margin: 3px;
/*opacity: 0.85;*/
width: 300px;
background: #37C058;
color: #fff;
padding: 8px;
border-radius: 5px;
-moz-border-radius: 5px;
-webkit-border-radius: 5px;
}
.notifyjs-corner .notifyjs-wrapper.error{background:#C03747;}